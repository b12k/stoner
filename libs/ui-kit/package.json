{
  "name": "@stoner/ui-kit",
  "version": "0.1.0",
  "license": "MIT",
  "main": "src/index.ts",
  "typings": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "serve storybook-static",
    "_dev": "tsdx watch",
    "build": "tsdx build",
    "docker-build": "env-cmd -i docker build {{CONTEXT}} --file={{DOCKERFILE}} --tag={{APP_NAME}} --build-arg=APP_NAME={{APP_NAME}}",
    "docker-build-ci": "env-cmd -i yarn docker-build --build-arg=BUILDKIT_INLINE_CACHE=1 --cache-from=localhost:5000/{{APP_NAME}}",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "storybook": "cross-env-shell \"start-storybook -p $UI_KIT_STORYBOOK_PORT\"",
    "storybook-build": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "7.17.0",
    "@storybook/addon-essentials": "6.4.18",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.4.18",
    "@storybook/addons": "6.4.18",
    "@storybook/react": "6.4.18",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "babel-loader": "8.2.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.5.5",
    "@stoner/docker-config": "*"
  },
  "dependencies": {
    "serve": "13.0.2"
  }
}
